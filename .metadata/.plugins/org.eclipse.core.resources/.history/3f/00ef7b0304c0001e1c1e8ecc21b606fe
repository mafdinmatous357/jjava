		Vector<double[]> body = new Vector<double[]>();
		double bodA[] = {20.0, 80.0};
		double bodB[] = {400.0, 500.0};
		body.add(bodA);
		body.add(bodB);
		g.drawLine(20, 80, 580, 80);
		for (int i=0;i<body.size();i++) {
			
		}
	}
}
